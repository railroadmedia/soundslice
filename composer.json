{
    "name": "railroad/soundslice",
    "type": "library",
    "description": "Soundslice Integration",
    "minimum-stability": "dev",
  "prefer-stable": true,
 "require": {
    "php": "~7",
    "laravel/framework": "5.6.*",
    "guzzlehttp/guzzle": "~6.0",
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "phpunit/php-code-coverage": "6.0.1",
    "orchestra/testbench": "~3.6",
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Soundslice\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Soundslice\\Tests\\": "tests/"
    }
  }
}
